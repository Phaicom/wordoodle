{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ESNext",
		"lib": [
			"DOM",
			"ESNext"
		],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true,
	},
}
